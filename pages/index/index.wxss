.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}


.homeimage{
  margin-left: 0.5rem;
}
.homesecond-custom{
  margin-top: 0.5rem;
}

.homesecond-item-text{
  color: white !important;
}

.homesecond-item-content1{
  border-radius: 10% !important;
  background-color: #E6A23C !important;
  box-shadow: 3px 3px 10px rgba(0,0,0,.5) !important;
}
.homesecond-item-content2{
  border-radius: 10% !important;
  background-color: #F56C6C !important;
  box-shadow: 3px 3px 10px rgba(0,0,0,.5) !important;
}
.homesecond-item-content3{
  border-radius: 10% !important;
  background-color: #909399 !important;
  box-shadow: 3px 3px 10px rgba(0,0,0,.5) !important;
}
.homefont{
  margin-left: 1rem;
  font-size: 15px;
}

/* 列表 */
.homethird-custom{
  text-align: left !important;
  margin-top: 0.5rem;
}
.homethird-item-content{
  background-color: #F2F6FC !important;
  border-radius: 10% !important;
}
.hometitle{
  font-size: 15px;
  text-align: left;
}
.homediscribe{
  color: #909399;
  font-size: x-small;
}
.homesupport{
  color: #909399;
  font-size: 10px;
  text-align: left;
  border: 1px;
  width: 4rem;
  float: left;
}
/*actionsheet*/
.row-custom{
  flex-direction: column;
  font-size: x-small;
}
.col-custom{
  width: 8rem;
}
.info-right{
  font-size: medium;
}
.card1{
  margin-top: 1rem;
  margin-bottom: 1rem;
}
.card2{
  width: 19rem;
  border-radius: 10% ;
  background-color: #F2F6FC;
  margin-left: 0.5rem;
  margin-bottom: 4.5rem;
  font-size: x-small;
  padding: 0.5rem;
}
.row{
  margin-left: 1rem;
  margin-bottom: 0.5rem;
}
.textright{
  float: right;
  margin-right: 0.5rem;
}
.btn{
  width: 40%;
  background-color:#7232dd;
  color: white;
}
.buttong{
  display: flex;
}